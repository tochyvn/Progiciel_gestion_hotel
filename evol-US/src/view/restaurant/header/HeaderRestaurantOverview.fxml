<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<FlowPane alignment="CENTER" columnHalignment="CENTER" prefHeight="84.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.restaurant.header.HeaderRestaurantController">
   <children>
      <Label fx:id="reservation" onMouseClicked="#showReservation" text="Reservations">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <FlowPane.margin>
            <Insets right="20.0" />
         </FlowPane.margin>
      </Label>
      <Label fx:id="stock" onMouseClicked="#showStock" text="Stocks">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <FlowPane.margin>
            <Insets right="20.0" />
         </FlowPane.margin>
      </Label>
   </children>
</FlowPane>
